{% extends "::Layout/base.html.twig" %}
{% block title %}{{'header.menu.directory'|trans }}{% endblock %}

{% block stylesheet %}
    <link rel="stylesheet" href="{{ asset('css/sliders/css3-panels/css3-panels.css') }}" type="text/css" media="all"/>
{% endblock %}
{% block body %}
    <div class="kl-slideshow kl-slideshow-css3panels" id="service">
        <div class="fake-loading loading-1s"></div>
        <div class="css3panels-container css3panels--resize cssp-capt-animated cssp-capt-slideout" data-panels="4" style="height:700px">
            <div class="css3panel css3panel--1 cp-theme--light">
                <div class="css3panel-inner" style=" height:700px ">
                    <div class="css3panel-mainimage-wrapper" style=" height:700px ">
                        <div style="background-image:url({{ asset('images/sliders/css3panel-011.jpg') }}); " class="css3panel-mainimage">
                            <div style="background-image:url({{ asset('images/sliders/css3panel-011.jpg') }}); " class="css3panel-mainimage css3panel-mainimage-effect anim--blur"></div>
                            <div class="css3p-overlay css3p-overlay--gradient"></div>    
                        </div>
                    </div>
                </div>
                <div class="css3panel-caption ">
                    <a href="#" target="_self"><h3 class="css3panel-title ff-alternative captiontitle--dbg title-size-normal">{{ 'header.menu.ph' |trans }}</h3></a>
                    <div class="css3panel-text">{{ 'patient.service.s1'|trans }}</div>
                    <div class="css3panel-btn-area">
                        <a href="{{ path('Patient_ServicePharmacy') }}" class="btn btn-fullcolor btn-skewed" target="_self">{{ 'patient.service.se'|trans }}</a>
                    </div>
                </div>  
            </div>
            <div class="css3panel css3panel--2 cp-theme--light">
