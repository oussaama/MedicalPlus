                        <div style="background-image:url({{ asset('images/sliders/css3panel-06.jpg') }}); " class="css3panel-mainimage">
                            <div style="background-image:url({{ asset('images/sliders/css3panel-06.jpg') }}); " class="css3panel-mainimage css3panel-mainimage-effect anim--blur"></div>
                            <div class="css3p-overlay css3p-overlay--gradient"></div>
                        </div>
                    </div>
                </div>
                <div class="css3panel-caption ">
                    <a href="#" target="_self">
                        <h3 class="css3panel-title ff-alternative captiontitle--dbg title-size-normal">{{ 'header.menu.be' |trans }}</h3>
                    </a>
                    <div class="css3panel-text">{{ 'patient.service.s2'|trans }}</div>
                    <div class="css3panel-btn-area">
                        <a href="{{ path('Patient_ServiceBeauty') }}" class="btn btn-fullcolor btn-skewed" target="_self">{{ 'patient.service.se'|trans }}</a>
                    </div>
                </div>
            </div>
            <div class="css3panel css3panel--3 cp-theme--light">
                <div class="css3panel-inner" style=" height:700px ">
                    <div class="css3panel-mainimage-wrapper" style=" height:700px ">
                        <div style="background-image:url({{ asset('images/sliders/css3panel-051.jpg') }});" class="css3panel-mainimage">
                            <div style="background-image:url({{ asset('images/sliders/css3panel-051.jpg') }}); " class="css3panel-mainimage css3panel-mainimage-effect anim--blur"></div>
                            <div class="css3p-overlay css3p-overlay--gradient"></div>
                        </div>
                    </div>
                </div>
                <div class="css3panel-caption ">
                    <h3 class="css3panel-title ff-alternative captiontitle--dbg title-size-normal">{{ 'header.menu.se' |trans }}</h3>
                    <div class="css3panel-text">{{ 'patient.service.s3'|trans }}</div>
                    <div class="more css3panel-btn-area">
                        <a class="btn btn-fullcolor btn-skewed" href="{{ path('Patient_ServiceClinical') }}" target="_self">{{ 'patient.slider.s3.i4'|trans }}</a>
                        <a class="btn btn-fullcolor btn-skewed" href="{{ path('Patient_ServiceHospital') }}" target="_self">{{ 'patient.slider.s3.i5'|trans }}</a>
                    </div>
                </div>
            </div>
            <div class="css3panel css3panel--4 cp-theme--light">
                <div class="css3panel-inner" style=" height:700px ">
                    <div class="css3panel-mainimage-wrapper" style=" height:700px ">
                        <div style="background-image:url({{ asset('images/bgHgSocialRegister.jpg') }}); " class="css3panel-mainimage">
                            <div style="background-image:url({{ asset('images/bgHgSocialRegister.jpg') }}); " class="css3panel-mainimage css3panel-mainimage-effect anim--blur"></div>
                            <div class="css3p-overlay css3p-overlay--gradient"></div>
                        </div>
                    </div>
                </div>
                <div class="css3panel-caption ">
                    <h3 class="css3panel-title ff-alternative captiontitle--dbg title-size-normal">{{ 'header.menu.m' |trans }}</h3>
                    <div class="css3panel-text">{{ 'patient.service.s4'|trans }}</div>
                    <div class="css3panel-btn-area">
                        <a href="{{ path('Patient_appointment') }}" class="btn btn-fullcolor btn-skewed" target="_self">{{ 'patient.service.se'|trans }}</a>
                    </div>
                </div>
            </div>
            <div class="css3panel css3panel--4 cp-theme--light">
                <div class="css3panel-inner" style=" height:700px ">
                    <div class="css3panel-mainimage-wrapper" style=" height:700px ">
                        <div style="background-image:url({{ asset('images/sliders/css3panel-031.jpg') }}); " class="css3panel-mainimage">
                            <div style="background-image:url({{ asset('images/sliders/css3panel-031.jpg') }}); " class="css3panel-mainimage css3panel-mainimage-effect anim--blur"></div>
                            <div class="css3p-overlay css3p-overlay--gradient"></div>
                        </div>
                    </div>
                </div>
                <div class="css3panel-caption ">
                    <h3 class="css3panel-title ff-alternative captiontitle--dbg title-size-normal">{{ 'header.menu.lab' |trans }}</h3>
                    <div class="css3panel-text">{{ 'patient.service.s5'|trans }}</div>
                    <div class="css3panel-btn-area">
                        <a href="{{ path('Patient_ServiceLaboratory') }}" class="btn btn-fullcolor btn-skewed" target="_self">{{ 'patient.service.se'|trans }}</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="kl-bottommask kl-bottommask--shadow_ud"></div>
    </div>
    <section class="hg_section"> 
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12">
                    <div class="kl-title-block clearfix tbk--left tbk-symbol--line tbk-icon-pos--after-title ptop-35 pbottom-65">
                        <h2 class="tbk__title fs-34 black montserrat"><strong>SERVICES</strong></h2>
                        <div class="tbk__symbol"><span></span></div>
                        <h4 class="tbk__subtitle fs-30 fw-thin">{{'patient.service.des'|trans}}</h4>
                    </div>
                </div>
            </div>
        </div>
    </section>

{% endblock %}

