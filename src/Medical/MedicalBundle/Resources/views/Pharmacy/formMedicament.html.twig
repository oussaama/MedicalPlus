<div class="col-sm-12" id="setting">
    <div class="ib2-style2 ib2-text-color-light-theme ib2-custom form-right shadow">
        <div class="contactForm">
            <form action="{{ path('AddPharmacy') }}" method="post" class="contact_form row" style="margin-top:-110px;" {{ form_enctype(form) }}>
                <div class="cf_response"></div>
                {{ form_start(form) }}
                <p class="col-sm-12 kl-fancy-form">
                    {{ form_label(form.REFERENCE)}}
                    <label class="control-label" style="margin:158px 0 0 -5px;">REFERENCE</label>
                </p> 
                <p class="col-sm-12 kl-fancy-form">
                    {{ form_label(form.NAME)}}
                    <label class="control-label" style="margin:208px 0 0 -5px;">NAME</label>
                </p> 
                <p class="col-sm-12 kl-fancy-form">
                    {{ form_label(form.CATEGORY)}}
                    <label class="control-label" style="margin:265px 0 0 -5px;">CATEGORY</label>
                </p> 
                <p class="col-sm-12 kl-fancy-form">
                    {{ form_label(form.STORAGE)}}
                    <label class="control-label" style="margin:320px 0 0 -5px;">STORAGE</label>
                </p>
                <p class="col-sm-12 kl-fancy-form">
                    {{ form_label(form.PRICE)}}
                    <label class="control-label" style="margin:375px 0 0 -5px;">PRICE</label>
                </p>
                <p class="col-sm-12 kl-fancy-form">
                    {{ form_label(form.PICTURE)}}
                </p>
                {{ form_end(form) }}
            </form>
        </div>
    </div>
</div>